{
  "name": "gdiapps",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "dtos": "cd src && x ts",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "build:local": "vue-tsc --noEmit && vite build --mode development --emptyOutDir",
    "publish": "npm run build && dotnet publish ../api/GDIApps/GDIApps.csproj -c Release",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "cd ../api/GDIApps && dotnet run --AppTasks=migrate",
    "migrate": "cd ../api/GDIApps && dotnet run GDIApps.csproj --AppTasks=migrate",
    "revert:last": "cd ../api/GDIApps && dotnet run --AppTasks=migrate.revert:last",
    "revert:all": "cd ../api/GDIApps && dotnet run --AppTasks=migrate.revert:all"
  },
